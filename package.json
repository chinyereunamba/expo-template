{
  "name": "expo-mobile-skeleton",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:dev": "node scripts/build-config.js validate development && eas build --profile development",
    "build:staging": "node scripts/build-config.js validate staging && eas build --profile preview",
    "build:prod": "node scripts/build-config.js validate production && eas build --profile production",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:staging": "eas submit --profile preview",
    "submit:prod": "eas submit --profile production",
    "update": "eas update",
    "update:dev": "eas update --channel development",
    "update:staging": "eas update --channel preview",
    "update:prod": "eas update --channel production",
    "config:validate": "node scripts/build-config.js validate",
    "config:list": "node scripts/build-config.js list",
    "setup:env": "node scripts/setup-env.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "debug:logs": "echo 'Check console for debug logs'",
    "debug:clear": "echo 'Debug data cleared - restart app to see changes'",
    "debug:export": "echo 'Use the debug screen in the app to export data'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@tanstack/query-async-storage-persister": "^5.83.1",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-query-persist-client": "^5.84.1",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "jwt-decode": "^4.0.0",
    "react": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.0.3",
    "yup": "^1.7.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@redux-devtools/extension": "^3.3.0",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "lint-staged": "^15.2.2",
    "msw": "^2.10.4",
    "prettier": "^3.2.5",
    "react-native-logs": "^5.3.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
